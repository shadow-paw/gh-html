{
  "name": "gh-html",
  "description": "Github Pages alike for private repository",
  "version": "0.0.1",
  "author": {
    "name": "shadow-paw"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shadow-paw/gh-html.git"
  },
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "dev": "NODE_ENV=development node -r dotenv/config dist/app.js",
    "prod": "NODE_ENV=production node -r dotenv/config dist/app.js",
    "build": "tsc",
    "rebuild": "npm run clean && tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "async": "3.2.3",
    "body-parser": "1.19.1",
    "connect-redis": "6.0.0",
    "dotenv": "14.3.2",
    "express": "4.17.2",
    "express-session": "1.17.2",
    "mime-types": "2.1.34",
    "query-string": "7.1.0",
    "request": "2.88.2",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@types/async": "3.2.12",
    "@types/body-parser": "1.19.2",
    "@types/connect-redis": "0.0.18",
    "@types/express": "4.17.2",
    "@types/express-session": "1.17.4",
    "@types/mime-types": "2.1.1",
    "@types/request": "2.48.8",
    "@types/qs": "6.9.7",
    "ts-node": "10.4.0",
    "tslint": "6.1.3",
    "tslint-microsoft-contrib": "6.2.0",
    "typescript": "4.5.5"
  }
}
