{
  "name": "gh-html",
  "description": "Github Pages alike for private repository",
  "version": "0.0.1",
  "author": {
    "name": "shadow-paw"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shadow-paw/gh-html.git"
  },
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "dev": "NODE_ENV=development node -r dotenv/config dist/app.js",
    "prod": "NODE_ENV=production node -r dotenv/config dist/app.js",
    "build": "tsc",
    "rebuild": "npm run clean && tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "async": "2.6.1",
    "body-parser": "1.18.3",
    "connect-redis": "3.4.0",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "mime-types": "2.1.21",
    "query-string": "6.2.0",
    "request": "2.88.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@types/async": "2.4.0",
    "@types/body-parser": "1.17.0",
    "@types/connect-redis": "0.0.8",
    "@types/dotenv": "6.1.0",
    "@types/express": "4.16.1",
    "@types/express-session": "1.15.11",
    "@types/mime-types": "2.1.0",
    "@types/query-string": "6.2.0",
    "@types/request": "2.48.1",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "typescript": "3.3.1"
  }
}
